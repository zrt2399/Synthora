    <BoxShadows x:Key="EmptyBoxShadow">none</BoxShadows>
    <Thickness x:Key="DefaultBoxShadowMargin">2</Thickness>
    <Thickness x:Key="DefaultBoxShadowParentMargin">-2</Thickness>

