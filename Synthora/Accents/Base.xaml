    <FontFamily x:Key="ContentControlThemeFontFamily">fonts:Inter#Inter, $Default</FontFamily>
    <Color x:Key="ThemeAccentColor">#DD007AFF</Color>
    <Color x:Key="ThemeAccentColor2">#AA007AFF</Color>
    <Color x:Key="ThemeAccentColor3">#77007AFF</Color>
    <Color x:Key="ThemeAccentColor4">#44007AFF</Color>
    <Color x:Key="ThemeForegroundLowColor">#FF808080</Color>
    <Color x:Key="ThemeForegroundMidColor">#FF707070</Color>
    <Color x:Key="HighlightForegroundColor">#FFFFFFFF</Color>
    <Color x:Key="ErrorColor">#FFFF0000</Color>
    <Color x:Key="ErrorLowColor">#10FF0000</Color>
    <Color x:Key="SuccessColor">#FF1F883D</Color>
    <Color x:Key="WarningColor">#FFDC9B28</Color>
    <Color x:Key="DangerColor">#FFDB3340</Color>
